predds1<-predict(m0.8.2,newdata=data.frame(ds=ds[32:61], massbxcx = massmea - masssd))
preddsm1<-predict(m0.8.2,newdata=data.frame(ds=ds[32:61], massbxcx = massmea + masssd))

plot(ds[32:61], sprich[32:61], col = colmassmono, pch = 19, main = "Monogamy", ylab = "Net diversification", xlab = "Synonymous subst rate", ylim = c(-3.5, 3))
