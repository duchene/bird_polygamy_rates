### Scatterplots of PGLS regressions

source("analysesMitMoltree3.Rscript")

yrPal <- colorRampPalette(c('yellow','red'))
colmasspoly <- yrPal(10)[as.numeric(cut(massbxcx[1:31],breaks = 10))]
colmassmono <- yrPal(10)[as.numeric(cut(massbxcx[32:61],breaks = 10))]

massmea <- mean(massbxcx)
masssd <- 2 * sd(massbxcx)

# For ds poly and mono

predds0<-predict(m0.8.1,newdata=data.frame(ds=ds[1:31], massbxcx = massmea))
predds1<-predict(m0.8.1,newdata=data.frame(ds=ds[1:31], massbxcx = massmea - masssd))
preddsm1<-predict(m0.8.1,newdata=data.frame(ds=ds[1:31], massbxcx = massmea + masssd))

plot(ds[1:31], sprich[1:31], col = colmasspoly, pch = 19, main = "Polygamy", ylab = "Net diversification", xlab = "Synonymous subst rate", ylim = c(-3.5, 3))
legend(-2.5, 1, legend = c("Mean mass", "Low mass"), lty = 1, lwd = 2, col = c("red", "yellow"), box.lwd = 0)
lines(ds[1:31], predds0, col="orange", lwd = 2)
lines(ds[1:31], predds1, col="yellow", lwd = 2)
lines(ds[1:31], preddsm1, col="red", lwd = 2)

